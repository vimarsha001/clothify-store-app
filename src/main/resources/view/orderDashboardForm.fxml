<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loadOrderDash" prefHeight="411.0" prefWidth="706.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OrderDashboardFormController">
   <children>
      <Pane layoutY="1.0" prefHeight="61.0" prefWidth="706.0" style="-fx-background-color: #FFFFFF;">
         <children>
            <Label layoutX="246.0" layoutY="14.0" prefHeight="37.0" prefWidth="214.0" text="Order Management" textFill="#503e9e">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
            <Label fx:id="dateLbl" layoutX="624.0" layoutY="24.0" text="Label">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <ImageView fx:id="backBtn" fitHeight="42.0" fitWidth="35.0" layoutX="20.0" layoutY="14.0" onMouseClicked="#backBtnOnAction" preserveRatio="true">
         <image>
            <Image url="@../images/left-arrow.png" />
         </image>
      </ImageView>
      <Pane layoutY="369.0" prefHeight="42.0" prefWidth="706.0" style="-fx-background-color: #514CDF;" />
      <JFXButton fx:id="cartBtn" buttonType="RAISED" layoutX="279.0" layoutY="206.0" onAction="#cartBtnOnAction" prefHeight="31.0" prefWidth="148.0" style="-fx-background-color: #464A91; -fx-background-radius: 10;" text="Cart " textFill="WHITE">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
         <effect>
            <Reflection />
         </effect>
      </JFXButton>
      <JFXButton fx:id="orderDetailsBtn" buttonType="RAISED" layoutX="279.0" layoutY="144.0" onAction="#orderDetailsBtnOnAction" prefHeight="31.0" prefWidth="148.0" style="-fx-background-color: #464A91; -fx-background-radius: 10;" text="Order Details" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
         <effect>
            <Reflection />
         </effect>
      </JFXButton>
   </children>
</AnchorPane>
