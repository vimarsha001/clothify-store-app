<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loadOrderDetails" prefHeight="411.0" prefWidth="706.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OrderDetailsFormController">
   <children>
      <Pane layoutY="1.0" prefHeight="61.0" prefWidth="706.0" style="-fx-background-color: #FFFFFF;">
         <children>
            <Label layoutX="246.0" layoutY="14.0" prefHeight="37.0" prefWidth="214.0" text="Order Management" textFill="#503e9e">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
            <Label fx:id="dateLbl" layoutX="624.0" layoutY="24.0" text="Label">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <ImageView fx:id="backBtn" fitHeight="42.0" fitWidth="35.0" layoutX="20.0" layoutY="14.0" onMouseClicked="#backBtnOnAction" preserveRatio="true">
         <image>
            <Image url="@../images/left-arrow.png" />
         </image>
      </ImageView>
      <Pane layoutY="369.0" prefHeight="42.0" prefWidth="706.0" style="-fx-background-color: #514CDF;" />
      <TableView fx:id="orderDetailsTbl" layoutX="254.0" layoutY="117.0" prefHeight="200.0" prefWidth="394.0">
        <columns>
          <TableColumn fx:id="prodIdCol" prefWidth="113.0" text="Product ID" />
          <TableColumn fx:id="qtyCol" prefWidth="155.0" text="Qty" />
            <TableColumn fx:id="totalCol" prefWidth="125.0" text="Total" />
        </columns>
      </TableView>
      <JFXComboBox fx:id="orderIdCmb" layoutX="38.0" layoutY="117.0" onAction="#orderIdCmbOnAction" prefHeight="25.0" prefWidth="116.0" promptText="Order ID" />
   </children>
</AnchorPane>
