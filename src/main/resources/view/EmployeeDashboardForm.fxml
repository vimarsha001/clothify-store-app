<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loadStaffDash" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EmployeeDashboardFormController">
   <children>
      <ImageView fx:id="backBtn" fitHeight="42.0" fitWidth="35.0" layoutX="21.0" layoutY="20.0" onMouseClicked="#backBtnOnAction" preserveRatio="true">
         <image>
            <Image url="@../images/left-arrow.png" />
         </image>
      </ImageView>
      <Label fx:id="dateLbl" layoutX="535.0" layoutY="29.0" text="Label">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <Pane layoutY="362.0" prefHeight="42.0" prefWidth="602.0" style="-fx-background-color: #514CDF;" />
      <ImageView fitHeight="35.0" fitWidth="28.0" layoutX="14.0" layoutY="319.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/profile.png" />
         </image>
      </ImageView>
      <Label fx:id="userLbl" layoutX="50.0" layoutY="325.0" text="Staff" textFill="#503e9e">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </Label>
      <JFXButton fx:id="productManBtn" buttonType="RAISED" layoutX="218.0" layoutY="123.0" onAction="#productManBtnOnAction" prefHeight="30.0" prefWidth="166.0" style="-fx-background-color: #503e9e; -fx-background-radius: 10;" text="Product Management" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="orderManBtn" buttonType="RAISED" layoutX="218.0" layoutY="187.0" onAction="#orderManBtnOnAction" prefHeight="30.0" prefWidth="166.0" style="-fx-background-color: #503e9e; -fx-background-radius: 10;" text="Order Management" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="supplierManBtn" buttonType="RAISED" layoutX="218.0" layoutY="247.0" onAction="#supplierManBtnOnAction" prefHeight="30.0" prefWidth="166.0" style="-fx-background-color: #503e9e; -fx-background-radius: 10;" text="Supplier Management" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
